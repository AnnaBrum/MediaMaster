{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/MenuItems/menuItems.ts",
    "app/home/(contact)/about/page.jsx",
    "app/auth/callback/route.js",
    "app/auth/sign-up/route.js",
    "app/auth/sign-in/route.js",
    "app/auth/sign-out/route.js",
    "app/settings/conditions/page.jsx",
    "app/create-account/register-with-email/verify-account/messages.jsx",
    "app/create-account/register-with-email/verify-account/page.jsx",
    "app/create-account/register-with-email/messages.jsx",
    "app/create-account/register-with-email/page.jsx",
    "app/create-account/messages.jsx",
    "app/create-account/page.jsx",
    "app/dbserv/page.jsx",
    "app/home/add-subscription/page.jsx",
    "app/home/(contact)/page.jsx",
    "app/home/my-subscriptions/page.jsx",
    "app/home/layout.jsx",
    "app/home/page.jsx",
    "app/login/messages.jsx",
    "app/login/page.jsx",
    "app/home/add-subscription/route.js",
    "app/route-handler/see-sub/route.js",
    "app/create-account/register-with-email/verify-account/(route).js",
    "app/server-page/page.jsx",
    "app/startpage/page.jsx",
    "app/layout.jsx",
    "app/page.jsx",
    "components/Button/Button.jsx",
    "components/CostSlider/CostSlider.jsx",
    "components/DbClient/DbClient.jsx",
    "components/DropDown/DropDown.jsx",
    "components/HamburgerMenu/HamburgerMenu.jsx",
    "components/Header/Header.jsx",
    "components/LogoutButton/LogoutButton.jsx",
    "components/MenuItems/MenuItems.jsx",
    "components/TotalCostSlider/TotalCostSlider.jsx"
  ],
  "exclude": ["node_modules"]
}
